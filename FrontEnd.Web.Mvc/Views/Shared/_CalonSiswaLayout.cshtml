<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />

    <title>CalonSiswa - @ViewBag.Title</title>

    <environment include="Development">
        <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet" />
        <link href="~/lib/startbootstrap-sb-admin-2/vendor/fontawesome-free/css/all.css" rel="stylesheet" />
        <link href="~/lib/startbootstrap-sb-admin-2/css/sb-admin-2.css" rel="stylesheet" />
    </environment>
    <environment exclude="Development">
        <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet" />
        <link href="~/lib/startbootstrap-sb-admin-2/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" />
        <link href="~/lib/startbootstrap-sb-admin-2/css/sb-admin-2.min.css" rel="stylesheet" />
    </environment>
</head>
<body id="page-top">
    <div id="wrapper">
        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-success sidebar sidebar-dark accordion" id="accordionSidebar">
            <li class="nav-item bg-white">
                <a class="sidebar-brand d-flex align-items-center justify-content-center"
                   asp-controller="CalonSiswa" asp-action="Index">
                    <div class="sidebar-brand-icon">
                        <img src="~/img/LogoSMABSS.Small.png" width="60" height="60"
                             asp-append-version="true" />
                    </div>
                    <div class="sidebar-brand-text text-gray-700 small mx-1">SMA Brawijaya Smart School</div>
                </a>
            </li>
            <li class="nav-item nav-link">
                <a class="nav-link" asp-controller="CalonSiswa" asp-action="Index">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Halaman Utama</span>
                </a>
            </li>
            <li><hr class="sidebar-divider" /></li>
            <li class="sidebar-heading">Biodata</li>
            <li class="nav-item">
                <a class="nav-link" asp-controller="CalonSiswa" asp-action="Biodata">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Biodata</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse"
                   data-target="#collapseKelolaBiodata" aria-expanded="true" aria-controls="collapseKelolaBiodata">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Kelola Biodata</span>
                </a>
                <div id="collapseKelolaBiodata" class="collapse" aria-label="headingKelolaBiodata" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <h6 class="collapse-header">Sesi:</h6>
                        <a class="collapse-item" asp-controller="CalonSiswa" asp-action="KelolaDataDiri">Data Diri</a>
                        <a class="collapse-item" asp-controller="CalonSiswa" asp-action="KelolaDataAkademikTerakhir">Data Akademik Terakhir</a>
                        <a class="collapse-item" asp-controller="CalonSiswa" asp-action="KelolaDataRapor">Data Rapor</a>
                        <a class="collapse-item" asp-controller="CalonSiswa" asp-action="KelolaDataPrestasi">Data Prestasi</a>
                        <a class="collapse-item" asp-controller="CalonSiswa" asp-action="KelolaDataOrangTua">Data Orang Tua & Wali</a>
                        <a class="collapse-item" asp-controller="CalonSiswa" asp-action="KelolaDataPenunjang">Data Penunjang</a>
                    </div>
                </div>
            </li>
            <li><hr class="sidebar-divider" /></li>
            <li class="sidebar-heading">Pengumuman</li>
            <li class="nav-item">
                <a class="nav-link" asp-controller="CalonSiswa" asp-action="HasilSeleksi">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Hasil Seleksi</span>
                </a>
            </li>
            <li><hr class="sidebar-divider" /></li>
            <li class="text-center d-none d-md-inline">
                <button id="sidebarToggle" class="rounded-circle border-0"></button>
            </li>
        </ul>
        <div id="content-wrapper" class="d-flex flex-column">
            <div id="content">
                <!-- Topbar -->
                <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">
                    <button id="sidebarToggleTop" class="btn btn-link d-md-none rounded-circle mr-3">
                        <i class="fa fa-bars"></i>
                    </button>
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item">
                            <a class="btn" asp-controller="Auth" asp-action="Logout">
                                Logout
                                <i class="fas fa-sign-out-alt fa-sm fa-fw text-gray-400"></i>
                            </a>
                        </li>
                    </ul>
                </nav>
                <!-- Content -->
                <div class="container-fluid">
                    <header class="d-sm-flex align-items-center justify-content-between mb-4">
                        <h1 class="h3 mb-0 text-gray-800">@ViewBag.Title</h1>
                        @RenderSection("Report", required: false)
                    </header>
                    <main class="mb-4">
                        @RenderBody()
                    </main>
                    <footer class="sticky-footer bg-white">
                        <div class="container my-auto">
                            <div class="copyright text-center my-auto">
                                <span>Copyright &copy; Your Website @DateTime.Now.Year</span>
                            </div>
                        </div>
                    </footer>
                </div>
            </div>
        </div>
    </div>
    <environment include="Development">
        <script src="~/lib/startbootstrap-sb-admin-2/vendor/jquery/jquery.js" type="text/javascript"></script>
        <script src="~/lib/startbootstrap-sb-admin-2/vendor/bootstrap/js/bootstrap.bundle.js" type="text/javascript"></script>
        <script src="~/lib/jquery-validation/jquery.validate.js"></script>
        <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
        <script src="~/lib/startbootstrap-sb-admin-2/js/sb-admin-2.js" type="text/javascript"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/lib/startbootstrap-sb-admin-2/vendor/jquery/jquery.min.js" type="text/javascript"></script>
        <script src="~/lib/startbootstrap-sb-admin-2/vendor/bootstrap/js/bootstrap.bundle.min.js" type="text/javascript"></script>
        <script src="~/lib/jquery-validation/jquery.validate.min.js"></script>
        <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
        <script src="~/lib/startbootstrap-sb-admin-2/js/sb-admin-2.min.js" type="text/javascript"></script>
    </environment>
    @RenderSection("Scripts", required: false)
</body>
</html>
